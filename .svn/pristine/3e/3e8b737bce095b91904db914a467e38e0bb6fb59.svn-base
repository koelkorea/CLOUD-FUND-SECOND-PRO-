package cmn;

/**
 * @author gockd
 *
 */
public class DTO {

	private int num;//글 번호
	
	private int totalCnt;//총 글 수
	
	
	//메세지 전달
	private int msgFlag;

	private String msg;
	
	
	
	public DTO() {}

	public int getNum() {
		return num;
	}

	public void setNum(int num) {
		this.num = num;
	}

	public int getTotalCnt() {
		return totalCnt;
	}

	public void setTotalCnt(int totalCnt) {
		this.totalCnt = totalCnt;
	}

	public int getMsgFlag() {
		return msgFlag;
	}

	public void setMsgFlag(int msgFlag) {
		this.msgFlag = msgFlag;
	}

	public String getMsg() {
		return msg;
	}

	public void setMsg(String msg) {
		this.msg = msg;
	}

	@Override
	public String toString() {
		return "DTO [num=" + num + ", totalCnt=" + totalCnt + ", msgFlag=" + msgFlag + ", msg=" + msg + "]";
	}

	
	
}
