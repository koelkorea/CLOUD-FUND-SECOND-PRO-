package vo;

import cmn.DTO;

public class ProjectVO extends DTO {

	private int p_seq;				// 프로젝트_순번
	private String p_m_id;			// 프로젝트_등록자ID
	private String p_title;			// 프로젝트_제목
	private String p_thumb;			// 프로젝트_대표이미지
	private String p_name;			// 프로젝트_등록자이름
	private String p_contents;		// 프로젝트_내용
	private String p_cate;			// 프로젝트_카테고리
	private String p_tag;			// 프로젝트_검색태그
	private String p_status;		// 프로젝트_진행상태
	private String p_opendate;		// 프로젝트_등록일
	private String p_closedate;		// 프로젝트_마감일
	private int p_minprice;			// 프로젝트_선물최소금액
	private String p_account;		// 프로젝트_후원입금계좌
	private int p_goalfund;			// 프로젝트_목표펀딩금액
	private String mod_id;			// 프로젝트_수정자ID
	private String mod_dt;			// 프로젝트_수정일
	
	public ProjectVO() {
		
	}

	public ProjectVO(int p_seq, String p_m_id, String p_title, String p_thumb, String p_name, String p_contents,
			String p_cate, String p_tag, String p_status, String p_opendate, String p_closedate, int p_minprice,
			String p_account, int p_goalfund, String mod_id, String mod_dt) {
		super();
		this.p_seq = p_seq;
		this.p_m_id = p_m_id;
		this.p_title = p_title;
		this.p_thumb = p_thumb;
		this.p_name = p_name;
		this.p_contents = p_contents;
		this.p_cate = p_cate;
		this.p_tag = p_tag;
		this.p_status = p_status;
		this.p_opendate = p_opendate;
		this.p_closedate = p_closedate;
		this.p_minprice = p_minprice;
		this.p_account = p_account;
		this.p_goalfund = p_goalfund;
		this.mod_id = mod_id;
		this.mod_dt = mod_dt;
	}
	
	// Getters and Setters
	public int getP_seq() {
		return p_seq;
	}

	public void setP_seq(int p_seq) {
		this.p_seq = p_seq;
	}

	public String getP_m_id() {
		return p_m_id;
	}

	public void setP_m_id(String p_m_id) {
		this.p_m_id = p_m_id;
	}

	public String getP_title() {
		return p_title;
	}

	public void setP_title(String p_title) {
		this.p_title = p_title;
	}

	public String getP_thumb() {
		return p_thumb;
	}

	public void setP_thumb(String p_thumb) {
		this.p_thumb = p_thumb;
	}

	public String getP_name() {
		return p_name;
	}

	public void setP_name(String p_name) {
		this.p_name = p_name;
	}

	public String getP_contents() {
		return p_contents;
	}

	public void setP_contents(String p_contents) {
		this.p_contents = p_contents;
	}

	public String getP_cate() {
		return p_cate;
	}

	public void setP_cate(String p_cate) {
		this.p_cate = p_cate;
	}

	public String getP_tag() {
		return p_tag;
	}

	public void setP_tag(String p_tag) {
		this.p_tag = p_tag;
	}

	public String getP_status() {
		return p_status;
	}

	public void setP_status(String p_status) {
		this.p_status = p_status;
	}

	public String getP_opendate() {
		return p_opendate;
	}

	public void setP_opendate(String p_opendate) {
		this.p_opendate = p_opendate;
	}

	public String getP_closedate() {
		return p_closedate;
	}

	public void setP_closedate(String p_closedate) {
		this.p_closedate = p_closedate;
	}

	public int getP_minprice() {
		return p_minprice;
	}

	public void setP_minprice(int p_minprice) {
		this.p_minprice = p_minprice;
	}

	public String getP_account() {
		return p_account;
	}

	public void setP_account(String p_account) {
		this.p_account = p_account;
	}

	public int getP_goalfund() {
		return p_goalfund;
	}

	public void setP_goalfund(int p_goalfund) {
		this.p_goalfund = p_goalfund;
	}

	public String getMod_id() {
		return mod_id;
	}

	public void setMod_id(String mod_id) {
		this.mod_id = mod_id;
	}

	public String getMod_dt() {
		return mod_dt;
	}

	public void setMod_dt(String mod_dt) {
		this.mod_dt = mod_dt;
	}

	// hashCode
	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((mod_dt == null) ? 0 : mod_dt.hashCode());
		result = prime * result + ((mod_id == null) ? 0 : mod_id.hashCode());
		result = prime * result + ((p_account == null) ? 0 : p_account.hashCode());
		result = prime * result + ((p_cate == null) ? 0 : p_cate.hashCode());
		result = prime * result + ((p_closedate == null) ? 0 : p_closedate.hashCode());
		result = prime * result + ((p_contents == null) ? 0 : p_contents.hashCode());
		result = prime * result + p_goalfund;
		result = prime * result + ((p_m_id == null) ? 0 : p_m_id.hashCode());
		result = prime * result + p_minprice;
		result = prime * result + ((p_name == null) ? 0 : p_name.hashCode());
		result = prime * result + ((p_opendate == null) ? 0 : p_opendate.hashCode());
		result = prime * result + p_seq;
		result = prime * result + ((p_status == null) ? 0 : p_status.hashCode());
		result = prime * result + ((p_tag == null) ? 0 : p_tag.hashCode());
		result = prime * result + ((p_thumb == null) ? 0 : p_thumb.hashCode());
		result = prime * result + ((p_title == null) ? 0 : p_title.hashCode());
		return result;
	}

	// equals
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		ProjectVO other = (ProjectVO) obj;
		if (mod_dt == null) {
			if (other.mod_dt != null)
				return false;
		} else if (!mod_dt.equals(other.mod_dt))
			return false;
		if (mod_id == null) {
			if (other.mod_id != null)
				return false;
		} else if (!mod_id.equals(other.mod_id))
			return false;
		if (p_account == null) {
			if (other.p_account != null)
				return false;
		} else if (!p_account.equals(other.p_account))
			return false;
		if (p_cate == null) {
			if (other.p_cate != null)
				return false;
		} else if (!p_cate.equals(other.p_cate))
			return false;
		if (p_closedate == null) {
			if (other.p_closedate != null)
				return false;
		} else if (!p_closedate.equals(other.p_closedate))
			return false;
		if (p_contents == null) {
			if (other.p_contents != null)
				return false;
		} else if (!p_contents.equals(other.p_contents))
			return false;
		if (p_goalfund != other.p_goalfund)
			return false;
		if (p_m_id == null) {
			if (other.p_m_id != null)
				return false;
		} else if (!p_m_id.equals(other.p_m_id))
			return false;
		if (p_minprice != other.p_minprice)
			return false;
		if (p_name == null) {
			if (other.p_name != null)
				return false;
		} else if (!p_name.equals(other.p_name))
			return false;
		if (p_opendate == null) {
			if (other.p_opendate != null)
				return false;
		} else if (!p_opendate.equals(other.p_opendate))
			return false;
		if (p_seq != other.p_seq)
			return false;
		if (p_status == null) {
			if (other.p_status != null)
				return false;
		} else if (!p_status.equals(other.p_status))
			return false;
		if (p_tag == null) {
			if (other.p_tag != null)
				return false;
		} else if (!p_tag.equals(other.p_tag))
			return false;
		if (p_thumb == null) {
			if (other.p_thumb != null)
				return false;
		} else if (!p_thumb.equals(other.p_thumb))
			return false;
		if (p_title == null) {
			if (other.p_title != null)
				return false;
		} else if (!p_title.equals(other.p_title))
			return false;
		return true;
	}

	// toString
	@Override
	public String toString() {
		return "ProjectVO [p_seq=" + p_seq + ", p_m_id=" + p_m_id + ", p_title=" + p_title + ", p_thumb=" + p_thumb
				+ ", p_name=" + p_name + ", p_contents=" + p_contents + ", p_cate=" + p_cate + ", p_tag=" + p_tag
				+ ", p_status=" + p_status + ", p_opendate=" + p_opendate + ", p_closedate=" + p_closedate
				+ ", p_minprice=" + p_minprice + ", p_account=" + p_account + ", p_goalfund=" + p_goalfund + ", mod_id="
				+ mod_id + ", mod_dt=" + mod_dt + ", toString()=" + super.toString() + "]";
	}
	
} // --- class
